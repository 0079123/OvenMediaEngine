<?xml version="1.0" encoding="UTF-8"?>
<Applications>
	<Application>
		<Name>app</Name>
		<!-- live, vod, liveedge, vodedge, ... -->
		<Type>live</Type>
		<Providers>
			<Provider>
				<!-- rtmp, webrtc, mpegts, ... -->
				<Type>rtmp</Type>
				<Decode-Ref>${ome.AppHome}/conf/Decode.xml</Decode-Ref>
			</Provider>
		</Providers>
		<Publishers>
			<Publisher>
				<!-- webrtc, hls, dash, rtmp, ... -->
				<Type>webrtc</Type>
				<Encodes-Ref>${ome.AppHome}/conf/Encodes.xml</Encodes-Ref>
			</Publisher>
		</Publishers>
	</Application>
</Applications>
